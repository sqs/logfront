GET     /api/applications           logfront.controllers.Applications.index

GET     /api/applications/:appName/environments    logfront.controllers.Environments.index(appName: String)

GET     /api/applications/:appName/environments/:envName/logs    logfront.controllers.Logs.index(appName: String, envName: String)

GET     /                           logfront.controllers.Application.index

GET     /assets/*file               controllers.Assets.at(path="/public", file)
